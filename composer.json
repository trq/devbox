{
    "name": "trq/devbox",
    "description": "A vagrant box designed to be installed a PHP project dependency via composer",
    "keywords": ["Ubuntu","Vagrant", "DevOps"],
    "homepage": "http://thorpesystems.com",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Tony R Quilkey",
        "email": "trq@thorpesystems.com",
        "homepage": "http://thorpesystems.com"
    }],
    "require": {
        "symfony/console": "~2.5.0"
    },
    "autoload": {
        "psr-4": {
            "Trq\\DevBox\\": ["src/"]
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.0.x-dev"
        }
    },
    "bin": ["bin/devbox"]
